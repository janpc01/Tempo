<!-- dashboard.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= dashboard.getName() %></title>
</head>
<body>
    <h1><%= dashboard.getName() %></h1>
    <!-- List Dashboards -->
    <ul>
        <% dashboard.getHabits().forEach(habit => { %>
            <li>
                <a href="/<%= authUser.username %>/<%= dashboard.getName() %>/<%= habit.getName() %>"><%= habit.getName() %></a>
            </li>
        <% }); %>
    </ul>
</body>
</html>
